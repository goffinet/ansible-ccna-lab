  name: "LANS"
  inside:
    - GigabitEthernet0/0
    - GigabitEthernet0/2
    - GigabitEthernet0/3
  outside: GigabitEthernet0/1
  sources:
    - "192.168.0.0 0.0.255.255"
    - "172.16.0.0 0.0.255.255"
