interfaces:
  - id: GigabitEthernet0/0
    description: "LAN R1"
    ipv4_address: 192.168.1.1/24
    rip:
    ipv6_addresses:
      - 'FE80::1'
      - 'FD00:FD00:FD00:1::1/64'
  - id: GigabitEthernet0/1
    description: "Internet connexion ip nat outside"
    ipv4_address: dhcp
  - id: GigabitEthernet0/2
    description: "Link to R2"
    ipv4_address: 192.168.225.1/24
    rip:
    ipv6_addresses:
      - 'FE80::1'
  - id: GigabitEthernet0/3
    description: "Link to R3"
    ipv4_address: 192.168.226.1/24
    rip:
    ipv6_addresses:
      - 'FE80::1'
routing:
  rid: 1.1.1.1
  passive:
    - GigabitEthernet0/0
  pid: 1
  as: 1
  rip:
    - 192.168.1.0
    - 192.168.225.0
    - 192.168.226.0
  ospfv2:
    - id: GigabitEthernet0/0
      ipv4: 192.168.1.1
      pri: 255
      cost: 1
      area: 0
    - id: GigabitEthernet0/2
      ipv4: 192.168.225.1
      pri: 255
      cost: 1
      area: 0
    - id: GigabitEthernet0/3
      ipv4: 192.168.226.1
      pri: 255
      cost: 1
      area: 0
  ospfv3:
    - id: GigabitEthernet0/0
      pri: 255
      cost: 1
      area: 0
    - id: GigabitEthernet0/2
      pri: 255
      cost: 1
      area: 0
    - id: GigabitEthernet0/3
      pri: 255
      cost: 1
      area: 0
  eigrp4:
    - id: GigabitEthernet0/0
      ipv4: 192.168.1.0
    - id: GigabitEthernet0/2
      ipv4: 192.168.225.0
    - id: GigabitEthernet0/3
      ipv4: 192.168.226.0
  eigrp6:
    - id: GigabitEthernet0/0
    - id: GigabitEthernet0/2
    - id: GigabitEthernet0/3
